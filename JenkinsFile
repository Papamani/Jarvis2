pipeline {
    agent any
    tools {
        maven "Maven"   
    }   
  
    stages {
        stage('Compile-Build-Test ') {
            steps {
                sh 'mvn clean package'
            }
        }
	 
	stage('Scan with Probely') {
      steps {
        probelyScan credentialsId: 'probely', targetId: '2Y4cg1YNq3nx'
      }
      }
      }
      }
